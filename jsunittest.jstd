server: http://localhost:4224

load:
  - assets/js/models/*.js
#  - assets/js/collections/*.js
#  - assets/js/views/*.js
#  - assets/js/*.js

test:
  - src-test/*.js

exclude:
 - assets/js/libs/*.js

serve:
 - assets/css/erp.css

proxy:
 - {matcher: "*", server: "http://erp.corthay.self.local/src-test/index.html"}

plugin:
 - name: "coverage"
   jar: "lib/jstestdriver/coverage.jar"
   module: "com.google.jstestdriver.coverage.CoverageModule"

timeout: 90